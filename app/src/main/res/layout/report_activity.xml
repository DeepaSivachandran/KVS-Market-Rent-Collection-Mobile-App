<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/comman_bg"
    android:orientation="vertical">
    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="@color/common_bar_bg"
        android:padding="4dp">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="2dp"
            android:padding="4dp"
            android:weightSum="2">

            <LinearLayout
                android:layout_weight="0.2"
                android:layout_width="0dp"
                android:gravity="left"
                android:layout_gravity="center"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/back_img"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:background="@drawable/ic_baseline_arrow_back_24">
                </ImageView>
            </LinearLayout>
            <TextView
                android:id="@+id/title_tv"
                android:layout_weight="1.1"
                android:layout_gravity="center"
                android:gravity="left"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:layout_marginStart="0dp"
                android:textSize="19dp"
                android:maxLines="1"
                android:ellipsize="end"
                android:text="Report "
                android:fontFamily="sans-serif"
                android:textStyle="bold">
            </TextView>

            <TextView
                android:id="@+id/date_tv"
                android:layout_weight="0.7"
                android:gravity="right"
                android:layout_gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textSize="19dp"
                android:text=""
                android:fontFamily="sans-serif"
                android:textStyle="bold">
            </TextView>




        </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>



    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:gravity="center"
        android:layout_below="@+id/app_bar"
        android:orientation="vertical"
        android:visibility="visible">
        <ListView
            android:id="@+id/report_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@null" />
        <ProgressBar
            android:id="@+id/progress_bar"
            android:layout_gravity="center"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:gravity="center"
            android:theme="@style/progressColor"
            android:background="@drawable/circular_progress_bar"
            android:visibility="gone">
        </ProgressBar>


    </FrameLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="0dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:weightSum="2"
        android:visibility="gone">

        <androidx.cardview.widget.CardView
            android:id="@+id/report_cv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="8dp"
            app:cardElevation="6dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="false">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="0dp"
                android:weightSum="2">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_weight="0.7"
                    android:background="#217b9c"
                    android:padding="2dp"
                    android:gravity="center">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="4dp"
                        android:text="Shift 1"
                        android:textColor="@color/white"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        tools:ignore="NestedWeights" />
                </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.3"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="0dp"
                android:weightSum="2">


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:padding="4dp"
                    android:gravity="center">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="Collection Date"
                        android:textColor="@color/common_txt_color"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        tools:ignore="NestedWeights" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="19-09-22"
                        android:textColor="#4f526b"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        tools:ignore="NestedWeights" />
                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="55dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="0"
                    android:background="@drawable/divider_bg"
                    android:gravity="center">
                </View>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="0dp"
                    android:layout_weight="1"
                    android:padding="4dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="Closing Time"
                        android:textColor="@color/common_txt_color"
                        android:textSize="14dp"
                        android:textStyle="normal"
                        tools:ignore="NestedWeights" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="2dp"
                        android:text="12:30"
                        android:textColor="#4b536b"
                        android:textSize="18dp"
                        android:textStyle="bold"
                        tools:ignore="NestedWeights" />
                </LinearLayout>

            </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:gravity="center"
        android:orientation="vertical"
        android:weightSum="2"
        android:visibility="gone">

        <androidx.cardview.widget.CardView
            android:id="@+id/report_card"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="8dp"
            app:cardElevation="6dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="0dp"
                android:gravity="center"
                android:orientation="vertical"
                android:weightSum="2">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="0dp"
                android:weightSum="2">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_weight="0.7"
                    android:background="@color/report_header"
                    android:padding="2dp"
                    android:gravity="center">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:fontFamily="sans-serif-condensed"
                        android:gravity="center"
                        android:padding="4dp"
                        android:text="Shift 2"
                        android:textColor="@color/white"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        tools:ignore="NestedWeights" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.3"
                    android:layout_marginTop="0dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp"
                    android:weightSum="2">


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:padding="2dp"
                        android:gravity="center">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:fontFamily="sans-serif-condensed"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="Collection Date"
                            android:textColor="@color/common_txt_color"
                            android:textSize="13dp"
                            android:textStyle="normal"
                            tools:ignore="NestedWeights" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:fontFamily="sans-serif-condensed"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="19-09-22"
                            android:textColor="#4f526b"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="55dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginEnd="0dp"
                        android:layout_weight="0"
                        android:background="@drawable/divider_bg"
                        android:gravity="center">
                    </View>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:layout_weight="1"
                        android:padding="2dp"
                        android:orientation="vertical"
                        android:gravity="center">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:fontFamily="sans-serif-condensed"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="Closing Time"
                            android:textColor="@color/common_txt_color"
                            android:textSize="13dp"
                            android:textStyle="normal"
                            tools:ignore="NestedWeights" />
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0"
                            android:fontFamily="sans-serif-condensed"
                            android:gravity="center"
                            android:padding="2dp"
                            android:text="01:30"
                            android:textColor="#4b536b"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            tools:ignore="NestedWeights" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/hidden_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="0dp"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="0dp"
                android:weightSum="2"
                android:visibility="gone">
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="0"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"
                    android:background="@drawable/divider_bg"
                    android:gravity="center">
                </View>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:layout_weight="0.7"
                    android:padding="2dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:background="@color/white"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="3dp"
                        android:weightSum="2">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:gravity="center"
                            android:padding="0dp"
                            android:weightSum="2">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:layout_weight="1"
                            android:padding="1dp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/customer_name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="start"
                                android:padding="0dp"
                                android:text="MariMuthu"
                                android:textColor="@color/common_txt_color"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                tools:ignore="NestedWeights" />

                            <TextView
                                android:id="@+id/acc_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="start"
                                android:padding="0dp"
                                android:text="A/C #19878800008"
                                android:textColor="@color/common_txt_color"
                                android:textSize="14dp"
                                android:textStyle="normal"
                                tools:ignore="NestedWeights" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:layout_weight="1"
                            android:gravity="end|center_vertical"
                            android:orientation="vertical"
                            android:padding="2dp">


                            <TextView
                                android:id="@+id/amount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center|center_vertical"
                                android:padding="1dp"
                                android:text="₹ 1200"
                                android:drawablePadding="2dp"
                                android:textColor="@color/common_txt_color"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                tools:ignore="NestedWeights" />

                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:layout_weight="0.7"
                    android:padding="2dp"
                    android:orientation="vertical"
                    android:gravity="center">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:background="@color/white"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="3dp"
                        android:weightSum="2">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:gravity="center"
                            android:padding="0dp"
                            android:weightSum="2">
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:layout_weight="1"
                            android:padding="1dp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="start"
                                android:padding="0dp"
                                android:text="Harish"
                                android:textColor="@color/common_txt_color"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                tools:ignore="NestedWeights" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="start"
                                android:padding="0dp"
                                android:text="A/C #19878800008"
                                android:textColor="@color/common_txt_color"
                                android:textSize="14dp"
                                android:textStyle="normal"
                                tools:ignore="NestedWeights" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginTop="0dp"
                            android:layout_weight="1"
                            android:gravity="end|center_vertical"
                            android:orientation="vertical"
                            android:padding="2dp">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_marginEnd="3dp"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center|center_vertical"
                                android:padding="1dp"
                                android:text="₹ 500"
                                android:drawablePadding="2dp"
                                android:textColor="@color/common_txt_color"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                tools:ignore="NestedWeights" />

                        </LinearLayout>


                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="0dp"
                    android:weightSum="2">


                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:padding="4dp"
                        android:gravity="center">
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="0dp"
                        android:layout_weight="1"
                        android:padding="2dp"
                        android:orientation="horizontal"
                        android:gravity="end">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="Total Collection"
                            android:fontFamily="sans-serif-condensed"
                            android:textColor="@color/common_txt_color"
                            android:layout_marginBottom="6dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="6dp"
                            android:textSize="15dp"
                            android:textStyle="normal" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="₹ 2000"
                            android:fontFamily="sans-serif-condensed"
                            android:textColor="@color/common_txt_color"
                            android:layout_marginBottom="6dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="6dp"
                            android:textSize="15dp"
                            android:textStyle="bold" />
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:padding="0dp"
                    android:weightSum="2">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:layout_weight="0"
                        android:orientation="horizontal"
                        android:padding="0dp"
                        android:gravity="start">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="500 * 2"
                            android:fontFamily="sans-serif-condensed"
                            android:textColor="@color/common_txt_color"
                            android:layout_marginBottom="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="2dp"
                            android:textSize="14dp"
                            android:textStyle="normal" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="35dp"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="= 2000"
                            android:fontFamily="sans-serif-condensed"
                            android:textColor="@color/common_txt_color"
                            android:layout_marginBottom="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_marginEnd="2dp"
                            android:textSize="14dp"
                            android:textStyle="bold" />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
